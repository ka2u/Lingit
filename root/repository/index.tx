[% WRAPPER "site/wrapper.tx" %]

<form action="repository/create">
  <label>Repository Path: <input type="text" name="path" /></label>
  <input type="submit" value="create" />
</form>

<div id="reposes">
<h2>Repositories</h2>
  <ul>
[% FOREACH repos IN reposes %]
    <li>* <a href="management/[% repos.rowid %]">[% repos.path %]</a></li>
[% END %]
  </ul>
</div>

[% END %]
